<p id="notice"><%= notice %></p>

<p>
  <strong>Description:</strong>
  <%= @photo.description %>
</p>

<p>
  <strong>Uploader:</strong>
  <%= @photo.uploader_id %>
</p>

<p>
  <strong>Image:</strong>
  <% @photo.image.each do |img|%>
  <%= image_tag(img.url) %>
  <% end %>
</p>

<%= link_to 'Edit', edit_photo_path(@photo) %> |
<%= link_to 'Back', photos_path %>
